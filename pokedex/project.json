{
  "name": "pokedex",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "pokedex/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project pokedex --web",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "configFile": "pokedex/vite.config.ts"
      },
      "outputs": ["{workspaceRoot}/dist/pokedex"]
    },
    "dev": {
      "executor": "@nx/vite:dev",
      "options": {
        "configFile": "pokedex/vite.config.ts"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "configFile": "pokedex/vite.config.ts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/pokedex",
        "configFile": "pokedex/vite.config.ts"
      }
    },
    "typecheck": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "tsc -p pokedex/tsconfig.app.json --noEmit && tsc -p pokedex/tsconfig.spec.json --noEmit"
      }
    }
  }
}
